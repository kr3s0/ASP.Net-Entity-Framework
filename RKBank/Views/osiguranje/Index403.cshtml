@model IEnumerable<RKBank.osiguranje>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.klijent.Ime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrojAktivnih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrojRizicnih)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BrojZavrsenih)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.klijent.Ime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrojAktivnih)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrojRizicnih)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BrojZavrsenih)
        </td>
        <td>
            
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
           
        </td>
    </tr>
}

</table>
